lexer grammar PgSQLLexerOperators;

// Logical Operators
OR          :   [Oo][Rr];
AND         :   [Aa][Nn][Dd];
NOT         :   [Nn][Oo][Tt];

IS          :   [Ii][Ss];
BETWEEN     :   [Bb][Ee][Tt][Ww][Ee][Ee][Nn];
IN          :   [Ii][Nn];

EQUAL       :   '=';
NOT_EQUAL   :   '!=';
NOT_EQUAL2  :   '<>';
ASSIGN      :   ':=';
PERCENT     :   '%';
ASTERISK    :   '*';
DEVIDE      :   '/';
